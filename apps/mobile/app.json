{
  "expo": {
    "name": "Cooked",
    "slug": "cooked",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "cooked",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cooked.app",
      "associatedDomains": [
        "applinks:cooked.app",
        "applinks:www.cooked.app"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D0D0D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cooked.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "cooked.app",
              "pathPrefix": "/group"
            },
            {
              "scheme": "https",
              "host": "cooked.app",
              "pathPrefix": "/join"
            },
            {
              "scheme": "https",
              "host": "cooked.app",
              "pathPrefix": "/recap"
            },
            {
              "scheme": "https",
              "host": "www.cooked.app",
              "pathPrefix": "/group"
            },
            {
              "scheme": "https",
              "host": "www.cooked.app",
              "pathPrefix": "/join"
            },
            {
              "scheme": "https",
              "host": "www.cooked.app",
              "pathPrefix": "/recap"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF4D00",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
